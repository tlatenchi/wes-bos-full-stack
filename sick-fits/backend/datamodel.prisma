type User {
  id: ID! @Unique @id
  name: String!
  email: String!
  updatedAt: DateTime! @updatedAt
  createdAt: DateTime! @createdAt
}
type Item {
  id: ID! @Unique @id
  title: String!
  description: String!
  image: String
  largeImage: String
  price: Int!
  updatedAt: DateTime! @updatedAt
  createdAt: DateTime! @createdAt
}